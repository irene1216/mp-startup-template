<navbar
  navStyle="{{navStyle}}"
  navHeight="{{navHeight}}"
  navTitle="{{navTitle}}"
/>

<tabbar
  langIndex="{{langIndex}}"
  language="{{language}}"
  tabItems="{{tabItems}}"
  navBot="{{navBot}}"
  bindtoIndex="toIndex"
  bindtoProfile="toProfile"
/>   

<index
  index="{{index}}"
  langIndex="{{langIndex}}"
  language="{{language}}"
  bindswitchLang="switchLang"
  navHeight="{{navHeight}}"
/>

<profile
  profile="{{profile}}"
  langIndex="{{langIndex}}"
  language="{{language}}"
  bindswitchLang="switchLang"
/>